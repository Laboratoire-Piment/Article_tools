@article{slopiecka_chemical_2022,
	title = {Chemical and physical characterization of food waste to improve its use in anaerobic digestion plants},
	volume = {5},
	issn = {2772-4271},
	url = {https://www.sciencedirect.com/science/article/pii/S2772427122000122},
	doi = {10.1016/j.nexus.2022.100049},
	abstract = {Considerable amounts of expired food waste are generated every day. They are rich in organic carbon and in other elements, including nitrogen, phosphorus and potassium, which cannot be wasted. The present work tested expired food waste in terms of biogas production efficiency in anaerobic digestion (AD) process. A database was extrapolated from the tests carried out in order to obtain a complete list of physico-chemical and biochemical methane potential (BMP) of 88 expired food waste. Many studies are based on the analysis of a small number of samples, which are don't present a complete picture of all the types of food waste. The organic composition and other factors such as pH, temperature, C/N ratio of the samples varies considerably with the region, the season and the processing characteristics, resulting in methane yield variations, ranging from 216 to 1476 mL CH4/gVS. Therefore, knowledge of the appropriate physical and chemical properties of the feedstock, working conditions and the effects of the inhibition of various components on the anaerobic digestion processes is a key element, necessary to optimize energy production from food waste.},
	language = {en},
	urldate = {2022-02-23},
	journal = {Energy Nexus},
	author = {Slopiecka, Katarzyna and Liberti, Federica and Massoli, Sara and Bartocci, Pietro and Fantozzi, Francesco},
	month = mar,
	year = {2022},
	keywords = {Anaerobic digestion process, BMP test, Biogas efficiency, Expired food characterization},
	pages = {100049},
	annote = {Infos : Caractérisation de différents dechets organiques afin d'améliorer leur méthanisation
Analyses : Experience et comparaison des résultats
Dechet : Dechets organiques venant de restaurant, hotel, etc
Notes : Déchets venant de restaurant, hotels etc., triés en catégories 88 types de nourriture caractérisé (pH, TS, VS, TOC, TKN, Fat, Protein, Carbohydrate, TP, TK, C/N et BMP)
Toutes les méthodes utilisés sont décrites
pH à 6.8-7.5 pour les aliments
Attention ratio C/N
Graisses produisent plus de biogaz en codigestion
La digestion anaérobie des aliments est une méthode viable mais aattention si elle est appliquée à des taux de charges organiques élevés (si juste déchets alimentaires par exemple), peut conduire à des processus instables.
La connaissance des propriétés physiques et chimiques appropriées de la matière première, des conditions de travail et des effets d'inhibition des différents composants est un élément clé, nécessaire pour un contrôle efficace du processus de digestion anaérobie},
